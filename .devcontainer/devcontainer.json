{
    "name": "Svelte Society",
    "dockerFile": "Dockerfile",
    "appPort": [
        3000
    ],
    "runArgs": [
        "-u",
        "node"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "postCreateCommand": "pnpm install",
    "extensions": [
        "esbenp.prettier-vscode",
        "svelte.svelte-vscode",
        "fivethree.vscode-svelte-snippets"
    ]
}